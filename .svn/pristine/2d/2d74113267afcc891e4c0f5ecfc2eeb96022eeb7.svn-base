<script type="text/javascript">
$(function(){
    $('body').attr('class', 'bg2');
    $('.div_scroll').scrollTop( $('#chat_data').height() );
    //如果用户在首页输入问题跳转直接显示用户问题
    if('{{ keyword }}'.length > 0){
        addUserSay('{{ keyword }}')
    }

    addSysSay('{{ keyword }}'); // 获取系统回答显示到页面

});//function

/*
添加系统返回答案
rawParam:keyword 为用户问题
 */
function addSysSay(keyword){
    //根据用户问题获取答案
    $.get('/get/search/data', {'keyword': keyword}, function(data){
        var lsSayHtml = data;
        setTimeout(function(){
            $('#chat_data').append(lsSayHtml);
            var last_div_h = $('#chat_data').children(':last').height();
            $('.div_scroll').scrollTop( $('#chat_data').height()+last_div_h );
            if('{{ keyword }}'.length > 0){
                location.href = '/indexchat?pattern=small';
            };
        }, 800);
    });
};

/*
用户输入问题显示到页面
 */
function addUserSay(keyword){
    var userSayHtml = '<div class="nr-xx"><Div class="nr-xxz"><div class="nr-xx1"><a class="tx">' +
                    '<img src="'+'{{ mine.photo|show_head('50x50', 'users') }}'+'" width="68" height="68" /></a></div>' +
                    '<div class="nr-xx3"></div></Div><Div class="nr-xx2">'+keyword+'</Div></div>';
    $('#chat_data').append(userSayHtml);
    var last_div_h = $('#chat_data').children(':last').height();
    $('.div_scroll').scrollTop( $('#chat_data').height()+last_div_h );
}
//发送按钮点击事件
function sendClick(){
    var keyword = $('#sayWord').val();
    addUserSay(keyword);
    addSysSay(keyword);
    $('#sayWord').val('');
};
//发送按钮点击事件
function sendEnterClick(e){
    if(e.keyCode == 13){
        sendClick();
    }
};
</script>
